{
  "name": "vue-rethinkdb",
  "version": "2.0.4",
  "description": "VueJS Plugin for RethinkDB",
  "author": "Chad Robinson",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crrobinson14/vue-rethinkdb"
  },
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npx bili --bundle-node-modules --format cjs --format umd --format esm --module-name RethinkDB",
    "lint": "eslint src",
    "pretest": "npm run lint"
  },
  "npmignore": [
    "coverage"
  ],
  "eslintIgnore": [
    "dist"
  ],
  "dependencies": {
    "reconnecting-websocket": "^4.1.10"
  },
  "devDependencies": {
    "bili": "^5.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-vue": "^2.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-vue": "^7.17.0",
    "rimraf": "^3.0.2",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14"
  }
}
